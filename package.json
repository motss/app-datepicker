{
  "name": "app-datepicker",
  "version": "4.0.0-rc.36",
  "description": "A custom datepicker element based on Google's Material Design built from scratch with lit-element",
  "keywords": [
    "cal",
    "calendar",
    "date",
    "date-picker",
    "datepicker",
    "lit-element",
    "lit-html",
    "picker",
    "web-component",
    "web-components"
  ],
  "homepage": "https://github.com/motss/app-datepicker",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/app-datepicker.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng (motss)",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "files": [
    "dist/*.*js.map",
    "dist/*.*js",
    "dist/*.d.ts.map",
    "dist/*.d.ts"
  ],
  "sideEffects": false,
  "main": "dist/app-datepicker.js",
  "module": "dist/app-datepicker.js",
  "typings": "dist/app-datepicker.d.ts",
  "scripts": {
    "build": "npm run ts -- -p tsconfig.prod.json",
    "clean": "shx rm -rf dist/ src/tests/snapshots/*.png",
    "document": "polymer analyze > dist/analysis.json",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish",
    "lint:debug": "tslint --config tslint.json --project tsconfig.json --format stylish",
    "prepublishOnly": "npm run lint && npm run build",
    "serve": "es-dev-server --root-dir . --port 3000 --node-resolve",
    "test": "concurrently \"npm run serve\" \"npm run wdio -- ./dist/tests/wdio.config.js\" --kill-others --kill-others-on-fail",
    "test:dev": "npm run build:test && npm t",
    "ts": "npm run clean && npm run lint && tsc",
    "tsc": "tsc",
    "upgrade": "npm outdated --dev; npm update --dev; npm audit fix",
    "watch": "npm run ts -- --watch",
    "wdio": "wdio",
    "build:test": "npm run ts -- -p tsconfig.test.json && tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run ts -- --noEmit && npm run clean"
    }
  },
  "dependencies": {
    "@material/mwc-button": "^0.11.1",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "nodemod": "^2.2.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@reallyland/really-elements": "^0.3.0",
    "@reallyland/tsconfig": "^1.0.1",
    "@reallyland/tslint-config": "^1.1.1",
    "@types/mocha": "^5.2.7",
    "@types/pretty": "^2.0.0",
    "@wdio/cli": "^5.16.15",
    "@wdio/local-runner": "^5.16.15",
    "@wdio/mocha-framework": "^5.16.15",
    "@wdio/sauce-service": "^5.16.10",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "axe-core": "3.3.0",
    "concurrently": "^5.0.1",
    "es-dev-server": "^1.31.1",
    "husky": "^3.1.0",
    "pretty": "^2.0.0",
    "pwa-helpers": "^0.9.1",
    "shx": "^0.3.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "engines": {
    "node": ">= 8.16.0",
    "npm": ">= 6.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
